{
    "name": "solvercircle/scbanner",
    "description": "It has a banner management panel to manage site banners.",
	"keywords": ["banner", "laravel", "carousel"],
    "authors": [
        {
            "name": "SolverCircle",
            "email": "solvercircle@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.2.*",
		"intervention/image": "1.*",
		"intervention/imagecache": "0.6.*"
    },
    "autoload": {
        "classmap": [
            "src/migrations",
			"src/controllers",
	        "src/models",
			"src/classes",
			"src/Solvercircle/Scbanner/Console"
        ],
        "psr-0": {
            "Solvercircle\\Scbanner\\": "src/"
        }
    },
    "minimum-stability": "dev"
}
